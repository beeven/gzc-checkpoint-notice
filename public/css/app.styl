.loading
  text-align center
  margin-top 10px

.loading-circular
  display inline-block
.loading-circular .circular {
  background-color: #5fb7ff;
  float: left;
  width: 15px;
  height: 15px;
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  border-radius: 10px;
  -webkit-animation: bounce_circular 0.7s infinite linear;
  animation: bounce_circular 0.7s infinite linear;
}
.loading-circular .circular:nth-child(1) {
  margin-top: 25px;
  -webkit-animation-delay: -1s;
}
.loading-circular .circular:nth-child(2) {
  margin-left: -8px;
  margin-top: 9px;
  -webkit-animation-delay: -0.9s;
}
.loading-circular .circular:nth-child(3) {
  margin-top: 1px;
  -webkit-animation-delay: -0.8s;
}
.loading-circular .circular:nth-child(4) {
  margin-left: 0;
  margin-top: 9px;
  -webkit-animation-delay: -0.7s;
}
.loading-circular .circular:nth-child(5) {
  margin-left: -8px;
  margin-top: 25px;
  -webkit-animation-delay: -0.6s;
}
.loading-circular .circular:nth-child(6) {
  margin-left: -22px;
  margin-top: 40px;
  -webkit-animation-delay: -0.5s;
}
.loading-circular .circular:nth-child(7) {
  margin-left: -37px;
  margin-top: 48px;
  -webkit-animation-delay: -0.4s;
}
.loading-circular .circular:nth-child(8) {
  margin-left: -53px;
  margin-top: 41px;
  -webkit-animation-delay: -0.3s;
}
@-webkit-keyframes bounce_circular {
  0% {
    -webkit-transform: scale(1);
  }
  100% {
    -webkit-transform: scale(0.3);
  }
}

.filter-form
  margin-top 10px

.filter-form input[type=search]
  -webkit-appearance:textfield
  &::-webkit-search-decoratoin
  &::-webkit-search-results-button
  &::-webkit-results-button,
  &::-webkit-results-decoration
    display inline


::-webkit-scrollbar
  display none


.status-board-item
  border-bottom 1px solid #aaa
  padding-left 5px
  padding-top 5px
  &:nth-child(even)
    background-color #ededed

.status-board-item-header
  font-weight bold
  position: relative
  .date
    position: absolute
    right: 45px